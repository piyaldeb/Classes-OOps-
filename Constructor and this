package Classes;

public class Constructor_and_this {
    public static class person{
        String name;
        int age;
        person(){
            //Default constructor
        }
        person(String name,int age){
            //parametrized
            this.name=name;
            this.age=age;
        }
        void print(){
            System.out.println(name +" "+ age);
        }
    }
    public static void main(String [] arg){
        person p1=new person();
        p1.name="Ranak";
        p1.age=21;
        person p2=new person("Piyal",20);//call and set the values to the constructor
        p1.print();
        p2.print();



    }
}
